{
  "name": "@singlestore/rag",
  "version": "0.0.6",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./chat": {
      "types": "./dist/chat/index.d.ts",
      "import": "./dist/chat/index.mjs",
      "require": "./dist/chat/index.js"
    },
    "./chat/tools": {
      "types": "./dist/chat/tools.d.ts",
      "import": "./dist/chat/tools.mjs",
      "require": "./dist/chat/tools.js"
    },
    "./session": {
      "types": "./dist/session.d.ts",
      "import": "./dist/session.mjs",
      "require": "./dist/session.js"
    },
    "./message": {
      "types": "./dist/message.d.ts",
      "import": "./dist/message.mjs",
      "require": "./dist/message.js"
    }
  },
  "scripts": {
    "build": "npm run clean && tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "lint": "eslint . "
  },
  "dependencies": {
    "@singlestore/ai": "*",
    "@singlestore/client": "*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/singlestore-labs/singlestore",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/singlestore-labs/singlestore.git"
  },
  "bugs": {
    "url": "https://github.com/singlestore-labs/singlestore/issues"
  },
  "keywords": [
    "singlestore"
  ]
}
